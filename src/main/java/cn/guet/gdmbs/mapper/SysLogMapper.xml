<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.guet.gdmbs.mapper.SysLogMapper">

    <insert id="saveSysLog" parameterType="SysLog">
        INSERT INTO sys_log(ip,username,operation,logtime,method) VALUES (#{ip},#{username},#{operation},#{logTime},#{method})
    </insert>
</mapper>